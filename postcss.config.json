{
	"plugins": [
		"postcss-flexbugs-fixes",
		[
			"@csstools/postcss-global-data",
			{ "files": ["src/app/styles/custom-media.css"] }
		],
		[
			"postcss-preset-env",
			{
				"autoprefixer": { "flexbox": "no-2009" },
				"stage": 3,
				"features": {
					"custom-media-queries": true,
					"custom-properties": false
				}
			}
		]
	]
}
